{
  "manifest_version": 2,
  "name": "restoreSubCount",
  "description": "restore the subscriber count on old.reddit",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://old.reddit.com/r/*"],
      "js": ["content_script.js"]
    }
  ]
}
